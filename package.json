{
  "name": "highway-hustle-blockchain",
  "version": "1.0.0",
  "description": "Highway Hustle Smart Contracts for 0G Mainnet",
  "scripts": {
    "compile": "hardhat compile",
    "deploy:testnet": "hardhat run scripts/deploy.js --network zerog_testnet",
    "deploy:mainnet": "hardhat run scripts/deploy-mainnet.js --network zerog_mainnet",
    "deploy:vehicle:mainnet": "hardhat run scripts/deploy_vehicle_manager.js --network zerog_mainnet",
