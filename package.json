    "deploy:score:mainnet": "hardhat run scripts/deploy-score-manager.js --network zerog_mainnet",
    "deploy:economy:mainnet": "hardhat run scripts/deploy-economy-manager.js --network zerog_mainnet",
    "estimate": "node scripts/estimate-costs.js",
